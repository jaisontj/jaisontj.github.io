{
	"version": "3.0.0",
	"name": "Jaison's Personal Website",
	"scripts": {
		"develop": "gatsby develop",
		"build": "gatsby build",
		"clean:public": "rm -rf public",
		"clean": "npm run clean:public",
		"lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'",
		"deploy": "gatsby build --prefix-paths && gh-pages -d public"
	},
	"author": "Jaison Titus",
	"license": "MIT",
	"dependencies": {
		"bluebird": "^3.5.1",
		"gatsby": "^2.0.22",
		"gatsby-plugin-google-analytics": "^2.0.6",
		"gatsby-plugin-google-fonts": "latest",
		"gatsby-plugin-offline": "^2.0.6",
		"gatsby-plugin-react-helmet": "^3.0.0",
		"gatsby-plugin-sass": "^2.0.1",
		"gatsby-plugin-sitemap": "^2.0.1",
		"lodash": "^4.17.4",
		"node-sass": "^4.9.3",
		"prismjs": "^1.15.0",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"react-helmet": "^5.2.0"
	},
	"devDependencies": {
		"babel-eslint": "^8.2.1",
		"eslint": "^4.14.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-prettier": "^3.1.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.5.1",
		"gh-pages": "^1.1.0",
		"lost": "^8.2.0",
		"postcss-pxtorem": "^4.0.1",
		"prettier": "^1.10.2"
	}
}
